@using EntityLayer.Concrete
@model List<Content>
@{
    ViewBag.Title = "GetAllList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("GetAllList", "Content", FormMethod.Post))
{
<p>
    Aranacak Kelimeyi Girin: @Html.TextBox("p")
